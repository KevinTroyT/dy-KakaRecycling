.zhuce{
	width: 100%;
	padding: 0 40rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.zhuce_ouput{
	width: 100%;
	font-size: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	height: 100rpx;
	line-height: 100rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	position: relative;
	border: 1px solid #ccc;
	border-radius: 100rpx;
	padding-left: 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	margin-bottom: 30rpx;
}
.zhuce_ouput>view{
    display: inline-block;
    text-align: right;
    color: rgb(33,33,33);
    width: 20%;
	float: left;
}
.zhuce_ouput>input{
    width: 360rpx;
}
