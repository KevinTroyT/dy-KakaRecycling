.commit-memu{
	width: 70%;
	height: 600rpx;
	position: fixed;
	left: 15%;
	top: 300rpx;
	border-radius: 20rpx;
	background-color: #fff;
	z-index: 999;
}
.commit-memu .title{
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 36rpx;
	text-align: center;
}
.commit-memu .close{
	width: 50rpx;
	height: 50rpx;
	position: absolute;
	top: 25rpx;
	right: 25rpx;
}
.commit-memu .memu-box{
	width: 100%;
	height: 420rpx;
	padding: 0 40rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.commit-memu .memu-box .title1{
	font-size: 32rpx;
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
}
.commit-memu .memu-box .memu-item{
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
}
.commit-memu .memu-box .memu-item>view{
	display: inline-block;
	font-size: 32rpx;
}
.commit-memu .memu-box .memu-item .memu-con{
	color: red;
	margin-left: 20rpx;
}
.commit-memu .bottom-btn{
	width: 100%;
	height: 80rpx;
}
.commit-memu .bottom-btn>view{
	display: inline-block;
	width: 50%;
	height: 100%;
	font-size: 36rpx;
	line-height: 80rpx;
	text-align: center;
}
.commit-memu .bottom-btn .memu-close{
	color: #ccc;
	background-color: #f2f2f2;
	border-bottom-left-radius: 20rpx;
}
.commit-memu .bottom-btn .commitCard{
	color: #fff;
